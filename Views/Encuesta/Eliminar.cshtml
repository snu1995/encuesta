
@model Encuesta





@using (Html.BeginForm())
{
     <div class="container-fluid mt-5">
	   <div classs="container mt-5">
		   <div class="card">
			   <div class="card-header bg-primary text-light">
					<h2>Eliminar Encuesta</h2>
			   </div>
			   <div class="card-body">
				   <p>¿Estás seguro de que deseas eliminar la encuesta "@Model.Nombre"?</p>

				   </div>
				   <div class="card-footer">
					    @Html.HiddenFor(model => model.Id)
					<button type="submit" class="mt-3 float-start btn btn-danger">Eliminar</button>
					<a href="@Url.Action("Index")" class="mt-3 float-end btn btn-secondary">Cancelar</a>
				   </div>
				   </div>
				   </div>
				   </div>
}
